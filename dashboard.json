          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH week_days AS (\n    SELECT \n        date_trunc('week', day) - INTERVAL '2 days' AS week_start,\n        COUNT(*) FILTER (WHERE day::date NOT IN (\n            '2025-02-06', '2025-02-07', '2025-02-09', '2025-02-10', \n            '2025-02-11', '2025-02-15', '2025-02-17', '2025-02-21',\n            '2025-02-22', '2025-03-01', '2025-03-02', '2025-03-03',\n            '2025-03-04', '2025-03-05', '2025-03-06', '2025-03-07',\n            '2025-03-08', '2025-03-09', '2025-03-10', '2025-03-11',\n            '2025-03-12', '2025-03-13', '2025-03-14', '2025-03-15',\n            '2025-03-16', '2025-03-23', '2025-03-24', '2025-03-29',\n            '2025-03-30', '2025-03-31', '2025-04-06', '2025-04-07'\n        )) AS active_days\n    FROM generate_series(\n        CURRENT_TIMESTAMP - INTERVAL '60 days', \n        CURRENT_TIMESTAMP, \n        INTERVAL '1 day'\n    ) AS day\n    GROUP BY date_trunc('week', day) - INTERVAL '2 days'\n)\nSELECT \n    TO_CHAR(date_trunc('week', t.due_date) - INTERVAL '2 days', 'MM/DD') AS week,\n    SUM(t.story_points) FILTER (WHERE t.is_action) / w.active_days::numeric AS completed_actions,\n    SUM(t.story_points) FILTER (WHERE NOT t.is_action) / w.active_days::numeric AS completed_tasks\nFROM tasks t\nJOIN week_days w ON date_trunc('week', t.due_date) - INTERVAL '2 days' = w.week_start\nWHERE t.due_date IS NOT NULL\n  AND t.due_date <= CURRENT_TIMESTAMP\n  AND t.due_date >= CURRENT_TIMESTAMP - INTERVAL '60 days'\n  AND DATE(t.due_date) NOT IN (\n    '2025-02-06', '2025-02-07', '2025-02-09', '2025-02-10', \n    '2025-02-11', '2025-02-15', '2025-02-17', '2025-02-21',\n    '2025-02-22', '2025-03-01', '2025-03-02', '2025-03-03',\n    '2025-03-04', '2025-03-05', '2025-03-06', '2025-03-07',\n    '2025-03-08', '2025-03-09', '2025-03-10', '2025-03-11',\n    '2025-03-12', '2025-03-13', '2025-03-14', '2025-03-15',\n    '2025-03-16', '2025-03-23', '2025-03-24', '2025-03-29',\n    '2025-03-30', '2025-03-31', '2025-04-06', '2025-04-07'\n  )\n  AND t.completion_date IS NOT NULL\nGROUP BY \n    date_trunc('week', t.due_date) - INTERVAL '2 days',\n    w.active_days\nORDER BY date_trunc('week', t.due_date) - INTERVAL '2 days' ASC;",
          "refId": "A",
